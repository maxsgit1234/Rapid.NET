<UserControl x:Class="Rapid.NET.Wpf.Editors.ArrayEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Rapid.NET.Wpf.Editors"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid HorizontalAlignment="Stretch">
        <DockPanel>
            <CheckBox Name="_ValChk" DockPanel.Dock="Left" VerticalAlignment="Center" ></CheckBox>
            <TextBox Name="_Value" DockPanel.Dock="Right" Margin="10,0,10,0"></TextBox>
        </DockPanel>
    </Grid>
</UserControl>
